{
  "name": "dostron",
  "version": "0.1.0",
  "description": "A cross-platform application to organise your collection of old programs for DOSBox.",
  "private": true,
  "main": "./dist-electron/index.js",
  "scripts": {
    "build": "vite build",
    "dev": "vite --host",
    "lint": "npm run lint:eslint && npm run lint:markdownlint && npm run lint:stylelint",
    "lint:eslint": "eslint . --ext .ts",
    "lint:markdownlint": "markdownlint *.md",
    "lint:stylelint": "stylelint ./src/**/*.{vue,css}",
    "prepare": "husky",
    "test": "vitest --run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garrett-he/dostron.git"
  },
  "keywords": [
    "dos",
    "dosbox",
    "electron"
  ],
  "author": "Garrett HE <garrett.he@outlook.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/garrett-he/dostron/issues"
  },
  "homepage": "https://github.com/garrett-he/dostron#readme",
  "dependencies": {
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.5",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.56.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "markdownlint-cli": "^0.39.0",
    "stylelint": "^16.3.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "^5.4.4",
    "vite": "^5.2.8",
    "vite-plugin-electron": "^0.28.5",
    "vitest": "^1.5.0"
  }
}
